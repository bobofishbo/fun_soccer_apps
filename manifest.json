{
    "manifest_version": 3,
    "name": "Liverpool are kings of Merseyside",
    "version": "1.0",
    "description": "Liverpool fans' personalized mental health protection tool. Make them think they are a good team on the internet.",
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js", "make-liverpool-first.js", "make-liverpool-perfect.js", "widget.js", "replace-liverpool-scores.js"],
        "run_at": "document_idle"
      }
    ],
    "action": {
      "default_title": "Liverpool are kings of Merseyside",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["icon.png"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  